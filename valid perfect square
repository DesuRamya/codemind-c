# codemind-c
#include<stdio.h>
int fun(int n)
{
    int j;
   for(j=1;j<=n;j++)
    {
        if(n==j*j)
        return 1;
    } 
    return 0;
}
int main()
{
    int n,i,j,a[100],r;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
       scanf("%d",&a[i]);
       r=fun(a[i]);
       if (r==1)
       printf("True\n");
       else
       printf("False\n");
    }
}
